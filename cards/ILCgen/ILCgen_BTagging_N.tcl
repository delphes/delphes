###################################
# b-tagging for inclusive jets, N=2
###################################
module BTagging BTagging80_N2 {
    set JetInputArray JetFinder_N2/jets
    set BitNumber 0

    source ILCgen/ILCgen_BTagging_80.tcl
}
module BTagging BTagging70_N2 {
    set JetInputArray JetFinder_N2/jets
    set BitNumber 1

    source ILCgen/ILCgen_BTagging_70.tcl
}
module BTagging BTagging50_N2 {
    set JetInputArray JetFinder_N2/jets
    set BitNumber 2

    source ILCgen/ILCgen_BTagging_50.tcl
}
